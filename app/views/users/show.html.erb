<h1>Your Profile</h1>
<h5>Logged in as <%= current_user.email %></h5>
<h3>Your Reviews</h3>
<ul>
  <% current_user.reviews.each do |review| %>
    <li>
      <p>You reviewed <%= link_to review.restaurant.name, restaurant_path(review.restaurant) %> on <%= review.updated_at.strftime("%b %d, %Y %I:%M %p") %> </p>
      <%=review.rating.to_s %> Star Rating: <%= review.content %>
      <p>
        <div style = "float: left">
          <%= button_to 'Edit', controller: 'reviews', action: 'edit', review_id: review.id, method: "get" %>
        </div>
        <div style = "float: left">
          <%= button_to 'Delete', {controller: 'reviews', action: 'destroy', review_id: review.id}, method: "delete" %>
        </div>
      </p>

  </li>
  <% end %>
</ul>
